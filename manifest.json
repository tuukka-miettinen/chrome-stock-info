{
	"manifest_version": 2,

	"name": "Stock Info",
	"description": "Provides context menu items to quickly check information on a stock based on it's ticker symbol.",
	"version": "0.0.1",
	"icons": {
		"16": "favicon-16x16.png",
		"32": "favicon-32x32.png",
		"96": "favicon-96x96.png"
	},

	"background": {
		"scripts": ["eventPage.js"]
	},
	"web_accessible_resources": [
		"symbols.json"
	],

	"permissions": [
		"storage",
		"contextMenus"
	]
}